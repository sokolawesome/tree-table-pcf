{
    "extends": "./node_modules/pcf-scripts/tsconfig_base.json",
    "compilerOptions": {
        "typeRoots": [
            "node_modules/@types"
        ],
        "jsx": "react",
        "module": "esnext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "outDir": "./out/controls",
        "target": "es5",
        "downlevelIteration": true,
        "lib": [
            "es2015",
            "dom"
        ],
    },
    "include": [
        "./TreeTable/**/*.ts",
        "./TreeTable/**/*.tsx"
    ],
    "exclude": [
        "node_modules"
    ]
}